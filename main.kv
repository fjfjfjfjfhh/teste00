<MDScreen>:
    name: "splash"


    # Start the progress bar animation when the screen is active
    on_enter: self.ids.progress.start()
    MDBoxLayout:
        md_bg_color: 0.10, 0.14, 0.18
        # Add float layout
        MDFloatLayout:
            md_bg_color: 0.10, 0.14, 0.18

            # Add Gif image
            Image:
                source:'app_icon_xxxhdpi.png'
                allow_stretch: True
                anim_delay: 0
                size_hint: (0.5, 0.5)
                pos_hint: {'center_x': 0.50, 'center_y': 0.75}
                # anim_loop: 1
                anim_reset: True

            MDLabel:
                text: "CARREGANDO DADOS..."
                pos_hint: {'center_x': 0.5, 'center_y': 0.45}
                halign: 'center'
                font_style: "H5"
                bold: True
                theme_text_color: 'Custom'
                text_color: 139,0,0
                font_size: "20sp"
            # Add box layout
            MDTextButton:
                pos_hint: {'center_x': .5, 'center_y': .38}
                text: 'Aguarde o carregamento de recursos.. \n'
                font_style: "Caption"
                text_color: (0.02, 0.83, 0.49)

            BoxLayout:
                # Set box position
                md_bg_color: 0.10, 0.14, 0.18
                pos_hint:{'center_y':.3, 'center_x':.5}
                padding: "10dp"
                # set width to 70%
                size_hint_x: .7





<LoginScreen>:
    name: "login_screen"
    font_name: 'Roboto'
    font_size: 60
    markup: True

    MDFloatLayout:
        orientation: "vertical"
        padding: 0,1
        md_bg_color: 0.10, 0.14, 0.18

        Image:
            source: "logo blazer off.png"
            allow_stretch: True
            size_hint: (0.5, 0.5)
            pos_hint: {'center_x': 0.50, 'center_y': 0.82}


        MDLabel:
            text: "Gestao e Qualidade"
            pos_hint: {'center_x': 0.5, 'center_y': 0.65}
            halign: 'center'
            font_style: "H5"
            bold: True
            theme_text_color: 'Custom'
            text_color: 139,0,0
            font_size: "20sp"

        MDTextButton:
            pos_hint: {'center_x': .5, 'center_y': .60}
            text: 'fa√ßa seu login \n'
            font_style: "Caption"
            text_color: (0.02, 0.83, 0.49)
            on_release: root.comprar_acesso()

        MDTextField:
            id: uname_input
            pos_hint: {'center_x': .5, 'center_y': .5}
            size_hint_x: .8
            icon_right: "email"
            hint_text: 'Usuario'
            mode: 'rectangle'
            color: (139,0,0)
            line_color_normal: (139,0,0)
            text_color: (.95,.44,.49,1)


        MDTextField:
            id: pword_input
            icon_right: "eye-off"
            password: True
            pos_hint: {'center_x': .5, 'center_y': .38}
            size_hint_x: .8
            hint_text: 'senha'
            mode: 'rectangle'
            color: (139,0,0)
            line_color_normal: (139,0,0)
            text_color: (.95,.44,.49,1)

        MDRectangleFlatButton:
            text: "Acessar"
            font_size: 20
            elevation: 3
            icon_right: "email"
            md_bg_color: 139, 0.1, 0
            text_color: (.1,.1,.1,1)
            line_color: (139,0,0)
            theme_text_color: 'Custom'
            pos_hint: {"center_x": .5, "center_y": .25}
            on_press: root.validate_user()

        MDTextButton:
            pos_hint: {'center_x': .5, 'center_y': .12}
            text: 'Adquirir login \n'
            font_style: "Caption"
            on_release: root.comprar_acesso()
            Widget:
                size_hint_y: .6






