<tela_4_digitos>:
    name: "tela_4_digitos"
    font_name: 'Roboto'
    font_size: 60
    markup: True

    ScrollView:
        line_color: (.95,.44,.49,1)
        size: self.size
        pos: self.pos


        MDFloatLayout:
            md_bg_color: 0.06, 0.10, 0.14
            orientation: "vertical"


            MDCard:
                orientation: "vertical"
                pos_hint: {'center_x': 0.50, 'center_y': 0.96}
                size_hint_y: .1
                md_bg_color: 0.95, 0.17, 0.30
                text_color: (255,255,255)
                line_color: (0.95, 0.17, 0.30)
                bold: True
                elevation: 3

                MDIconButton:
                    size_hint_x: .1
                    icon: "keyboard-backspace"
                    on_press: app.root.current = "Tela_PrincipalScreen"
                    on_press: root.desativar_jogo_4_digitos()
                    focus_color: app.theme_cls.accent_color
                    unfocus_color: app.theme_cls.primary_color

            MDLabel:
                halign: "center"
                text: "sistema de analises Quatro (4) digitos"
                theme_text_color: 'Custom'
                text_color: 255,0,0
                elevation: 3
                font_size: "16sp"
                pos_hint: {'center_x': 0.50, 'center_y': 0.95}

            Image:
                source: "app_icon_xxxhdpi.png"
                pos_hint: {'center_x': 0.5, 'center_y': 0.78}
                size_hint: (0.5, 0.2)
                allow_stretch: True
                elevation: 3


            ButtonFocus:
                bold: True
                elevation: 3
                # text: 'Start'
                pos_hint: {'center_x': 0.12, 'center_y': 0.74}
                text: 'Historico'
                font_size: 12
                halign: "center"
                md_bg_color: 0.95, 0.17, 0.30
                text_color: (255,255,255)
                line_color: (0.95, 0.17, 0.30)
                theme_text_color: 'Custom'
                focus_color: app.theme_cls.accent_color
                unfocus_color: app.theme_cls.primary_color
                on_press: root.historico()

            ButtonFocus:
                pos_hint: {'center_x': 0.88, 'center_y': 0.74}
                text: 'vitorias'
                font_size: 12
                halign: "center"
                md_bg_color: 0.95, 0.17, 0.30
                text_color: (255,255,255)
                line_color: (0.95, 0.17, 0.30)
                bold: True
                elevation: 3
                theme_text_color: 'Custom'
                focus_color: app.theme_cls.accent_color
                unfocus_color: app.theme_cls.primary_color
                on_press: root.historico()






            MDLabel:
                pos_hint: {'center_x': 0.5, 'center_y': 0.62}
                halign: "center"
                text: "Tempo de Analise"
                theme_text_color: 'Custom'
                bold: True
                text_color: .61, 0.68, 0.69
                font_size: "12sp"



            MDLabel:
                pos_hint: {'center_x': 0.5, 'center_y': 0.58}
                id: stopwatch
                markup: True
                halign: "center"
                text: '00:00.[size=40]00[/size]'
                elevation: 3
                theme_text_color: 'Custom'
                text_color: .61, 0.68, 0.69
                font_size: "14sp"


            MDBoxLayout:
                pos_hint: {'center_x': 0.5, 'center_y': 0.52}
                orientation: "horizontal"
                MDLabel:
                    halign: "center"
                    text: "Ultima Cor"
                    theme_text_color: 'Custom'
                    bold: True
                    text_color: .61, 0.68, 0.69
                    font_size: "12sp"


                MDLabel:
                    id: resultado_cor
                    halign: "center"
                    text: "aguardando"
                    theme_text_color: 'Custom'
                    bold: True
                    text_color: .61, 0.68, 0.69
                    font_size: "14sp"

            MDBoxLayout:
                pos_hint: {'center_x': 0.5, 'center_y': 0.43}
                orientation: "horizontal"
                MDLabel:
                    halign: "center"
                    text: "Ultimo Numero"
                    theme_text_color: 'Custom'
                    bold: True
                    text_color: .61, 0.68, 0.69
                    font_size: "12sp"


                MDLabel:
                    id: resultado
                    halign: "center"
                    text_autoupdate: True
                    bold: True
                    text: "Aguardando"
                    theme_text_color: 'Custom'
                    text_color: .61, 0.68, 0.69
                    font_size: "14sp"


            MDBoxLayout:
                pos_hint: {'center_x': 0.5, 'center_y': 0.35}
                orientation: "vertical"
                padding: 0,1

                MDLabel:
                    pos_hint: {'center_x': 0.5, 'center_y': 0.22}
                    halign: "center"
                    text: "GIROS ANTERIORES"
                    theme_text_color: 'Custom'
